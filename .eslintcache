ients} />\r\n        <BuildControls\r\n         addIngredient={this.addIngredient}\r\n         removeIngredient={this.removeIngredient}\r\n         disableIngredients={disableIngredients}\r\n         price={this.state.bill}\r\n         purchase={this.state.purchase}\r\n         purchase={this.state.purchaseable}\r\n         onPurchase={this.onPurchase}\r\n        />\r\n    </Aux>\r\n\r\n      );\r\n\r\n    }\r\n}\r\n\r\nexport default BurgerBuilder;","C:\\Users\\oluad\\OneDrive\\Documents\\GitHub\\FASTFOOD\\src\\components\\Layout\\Layout.js",["88"],"import React, { Component } from 'react';\r\nimport Aux from '../../containers/hoc/Auxx';\r\nimport classes from '../../components/Layout/Layout.module.css';\r\nimport Toolbar from '../Navigation/Toolbar/Toolbar';\r\n\r\n\r\nconst layout = (props) =>  (\r\n    <Aux>\r\n    <Toolbar></Toolbar>\r\n    <div>Toolbar, SideDrawer, Backdrop</div>\r\n    <main className={classes.Content}>\r\n        {props.children}\r\n    </main>  \r\n    </Aux>\r\n    \r\n\r\n)\r\n\r\nexport default layout;","C:\\Users\\oluad\\OneDrive\\Documents\\GitHub\\FASTFOOD\\src\\containers\\hoc\\Auxx.js",[],"C:\\Users\\oluad\\OneDrive\\Documents\\GitHub\\FASTFOOD\\src\\components\\Burger\\Burger.js",["89","90"],"import React, { Component } from 'react';\r\nimport classes from '../Burger/Burger.module.css';\r\n\r\nimport BurgerIngredient from './BurgerIngredients/BurgerIngredients';\r\n\r\nconst burger = (props) => {\r\n\r\n    \r\n\r\n    let transformedIngredients = Object.keys(props.type)\r\n\r\n    .map( igkey =>{ \r\n       return [...Array(props.type[igkey])].map((_,i) => {\r\n       return <BurgerIngredient key={igkey+i} type={igkey}   />\r\n    })\r\n\r\n   \r\n\r\n}).reduce((t,c)=>{ return t.concat(c)}) \r\n\r\nif (transformedIngredients.length == 0) {\r\n    transformedIngredients = <p>Please add Ingredients</p>\r\n    \r\n}\r\n\r\n\r\n\r\nreturn (\r\n\r\n  \r\n\r\n    <div  id=\"test \"className={classes.Burger}>\r\n        <BurgerIngredient